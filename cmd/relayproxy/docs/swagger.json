                "PROVIDER_NOT_READY",
                "FLAG_NOT_FOUND",
                "PARSE_ERROR",
                "TYPE_MISMATCH",
                "GENERAL",
                "INVALID_CONTEXT",
                "TARGETING_KEY_MISSING",
                "FLAG_CONFIG"
            ],
            "x-enum-varnames": [
